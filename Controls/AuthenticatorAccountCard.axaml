<UserControl x:Class="Authenticator.Controls.AuthenticatorAccountCard"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="using:Authenticator.ViewModels"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:DataType="vm:AuthenticatorAccountViewModel"
    mc:Ignorable="d">

  <Design.DataContext>
    <vm:AuthenticatorAccountViewModel />
  </Design.DataContext>

  <StackPanel>
    <TextBlock Text="{Binding Issuer}" />
  </StackPanel>
</UserControl>
